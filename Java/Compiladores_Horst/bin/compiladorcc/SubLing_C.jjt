* @post-condition: express�es matem�tica e booleanas s�o formadas
*/
void expressao(RecoverySet g) #EXPRESSAO: { }
{
  try { 
	numexpr() [( < LT > | < GT > | < LE > | < GE > | < EQ > | < NEQ > | < AND > | < OR > ) numexpr()]
	}catch(ParseException e) {
	  consumeUntil(g, e, "expressao");
	}
}

/**
